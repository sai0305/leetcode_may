//used hashmap, time complexity:o(1),space complexity:o(n);

class Solution {
public:
    int numJewelsInStones(string j, string s) {
        
        
        	int a[60]={0};
	for(int i=0;i<j.size();i++)
	{
		a[j[i]-'A']=1;
		
		
	}int count=0;
	for(int i=0;i<s.size();i++)
	{
		if(a[s[i]-'A']==1)
		count++;
		
		
		
	}return count;
        
    }
};
